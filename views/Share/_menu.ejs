<header>
	<div class="clearfix">
		<div id="LeftMenu">
			<a href="javascript:;" id="MenuBtn" class=""><span>left memu</span></a>
			<a href="javascript:;" id="MbMenuBtn" class=""><span>left memu</span></a>
		</div>
		<a href="/" id="Logo"><span>360 shopping channel</span></a>
		
		<div class="hdRight clearfix">
			<ul id="Menu" class=" clearfix">
				<li class="mobile mbCart"><a href="/cart" class="mobileCart"><span class="cartConut" data-val="<%- cartNumb?cartNumb:0 %>"><%- cartNumb?cartNumb:0 %></span></a></li>
			 <% menu.forEach(function(m,k){ %>
			 <% if(k>2){%>
					<li class="<%- m['multi'] ? 'multi':'' %>">
						<% if(m['multi']){%>
							<a href='<%= m[0] %>' class="mLink hasSub"><%= m['multi'][lgkey] %></a>
							<div class="subBox">
								<div class="subArrow">subArrow</div>
						    <ul class="subMenu">
						      <% m['sublevel'].forEach(function(sm,key){ %>
						      <li><a href="<%= sm[0] %>"><span><%= sm[lgkey] %></span></a></li>
						      <% }) %>
						    </ul>
					    </div>
				 		<% }else{ %>
							<a href="<%= m[0] %>" class="mLink <%- m[0]=="/cart" ? "cartLink1":"" %>"><span><%= m[lgkey] %></span></a>
				 		<% }%>
						<% if(m[0]=="/cart"){%>
				 		<span class="cartConut" id="CartWarp" data-val="<%- cartNumb?cartNumb:0 %>"><%- cartNumb?cartNumb:0 %></span>
						<% } %>
				 		<span class="mCut">|</span>
					</li>
			 <% }}) %>
			</ul>
			<div class="searchBox">
					<div class="iptBlock">
						<input type="text" value="" class="iptSearch" placeholder="<%=langs['sch_want'][lgkey] %>">
					</div>
					<a href="javascript:;" class="btns searchBtn"><span><%=langs['search'][lgkey] %></span></a>
			</div>
		</div>
	</div>
	<% include _mobileMenu.ejs %>
</header>
<div id="SlideMenu">
	<div class="sldMBox">
		<ul>
			 <% menu.forEach(function(m,k){ %>
			 <% if(k<=2){%>
			<li>
				<a href="#" class="mLink <%= k==0?'active':'' %>"><span><%= m[lgkey] %></span></a>
			</li>
			 <% }}) %>
		</ul>
	</div>
</div>
<div id="PageMain">